    "@modelcontextprotocol/sdk": "^1.15.1",
    "openapi-utils": "workspace:*"
  },
  "peerDependencies": {
    "@modelcontextprotocol/sdk": "^1.15.1"
  },
  "dependencies": {
    "zod": "=3.25.76"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
