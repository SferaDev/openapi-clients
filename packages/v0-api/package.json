{
  "name": "v0-api",
  "version": "0.0.4",
  "description": "Vercel v0 auto-generated type-safe API client",
  "author": "SferaDev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SferaDev/openapi-clients/issues"
  },
  "homepage": "https://github.com/SferaDev/openapi-clients#readme",
  "scripts": {
    "tsc": "tsgo --noEmit",
    "build": "unbuild",
    "generate": "rimraf ./src/generated && kubb generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SferaDev/openapi-clients.git"
  },
  "devDependencies": {
    "@kubb/cli": "^3.16.0",
    "@kubb/core": "^3.16.0",
    "@kubb/plugin-zod": "^3.16.0",
    "@kubb/react": "^3.16.0",
    "@modelcontextprotocol/sdk": "^1.16.0",
    "openapi-utils": "workspace:*"
  },
  "peerDependencies": {
    "@modelcontextprotocol/sdk": "^1.15.1"
  },
  "dependencies": {
    "zod": "=3.25.76"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./mcp": {
      "development": "./src/mcp.ts",
      "import": {
        "types": "./dist/mcp.d.mts",
        "default": "./dist/mcp.mjs"
      },
      "require": {
        "types": "./dist/mcp.d.cts",
        "default": "./dist/mcp.cjs"
      }
    }
  }
}
