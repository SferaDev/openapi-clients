{
  "name": "vercel-api-js",
  "version": "1.0.0",
  "description": "Vercel auto-generated type-safe API client",
  "author": "SferaDev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SferaDev/openapi-clients/issues"
  },
  "homepage": "https://github.com/SferaDev/openapi-clients#readme",
  "zshy": {
    "exports": {
      ".": "./src/index.ts",
      "./mcp": "./src/mcp.ts"
    }
  },
  "scripts": {
    "tsc": "tsc --noEmit",
    "build": "zshy",
    "generate": "rimraf ./src/generated && kubb generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SferaDev/openapi-clients.git"
  },
  "devDependencies": {
    "@kubb/cli": "^3.16.0",
    "@kubb/core": "^3.16.0",
    "@modelcontextprotocol/sdk": "^1.16.0",
    "openapi-utils": "workspace:*"
  },
  "peerDependencies": {
    "@modelcontextprotocol/sdk": "^1.15.1"
  },
  "dependencies": {
    "zod": "=3.25.76"
  },
  "files": ["dist"],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./mcp": {
      "types": "./dist/mcp.d.ts",
      "import": "./dist/mcp.mjs",
      "require": "./dist/mcp.js"
    }
  }
}
