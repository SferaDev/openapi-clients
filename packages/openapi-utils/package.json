{
  "name": "openapi-utils",
  "version": "0.0.1",
  "description": "Utilities for OpenAPI clients",
  "private": "true",
  "author": "SferaDev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SferaDev/openapi-clients/issues"
  },
  "homepage": "https://github.com/SferaDev/openapi-clients#readme",
  "scripts": {
    "tsc": "tsgo --noEmit",
    "build": "unbuild"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SferaDev/openapi-clients.git"
  },
  "dependencies": {
    "@kubb/core": "^4.0.0",
    "@kubb/oas": "^4.0.0",
    "@kubb/plugin-client": "^4.0.0",
    "@kubb/plugin-mcp": "^4.0.0",
    "@kubb/plugin-oas": "^4.0.0",
    "@kubb/plugin-ts": "^4.0.0",
    "@kubb/plugin-zod": "^4.0.0",
    "@kubb/react": "^4.0.0",
    "case": "^1.6.3"
  },
  "devDependencies": {
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "files": ["dist"],
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  }
